-- Here Your Toggle For Kavo Ui

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Ferine", "DarkTheme")
local MainTab = Window:NewTab("Main")
local Section = MainTab:NewSection("Main Tab")

Section:NewButton("instant open doors", "open doors in a instant", function()
    local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Function to instantly complete the ProximityPrompt
local function instantlyCompletePrompt(prompt)
    if prompt and prompt:IsA("ProximityPrompt") then
        -- Fire the ProximityPrompt's Triggered event to complete the interaction
        fireproximityprompt(prompt)
    end
end

-- Monitor for when ProximityPrompt is interacted with
local function monitorPrompts(object)
    if object:IsA("ProximityPrompt") then
        -- Detect when the player clicks the proximity prompt
        object.Triggered:Connect(function()
            instantlyCompletePrompt(object)
        end)
    end
end

-- Listen for any new objects added to the workspace
workspace.DescendantAdded:Connect(function(descendant)
    monitorPrompts(descendant)
end)

-- Also check existing objects in the workspace
for _, descendant in ipairs(workspace:GetDescendants()) do
    monitorPrompts(descendant)
end
end)

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ScreenGui"
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false

local Toggle = Instance.new("TextButton")
Toggle.Name = "Toggle"
Toggle.Parent = ScreenGui
Toggle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Toggle.Position = UDim2.new(0, 0, 0.454706937, 0)
Toggle.Size = UDim2.new(0, 90, 0, 38)
Toggle.Font = Enum.Font.SourceSans
Toggle.Text = "Toggle"
Toggle.TextColor3 = Color3.fromRGB(248, 248, 248)
Toggle.TextSize = 28.000
Toggle.Draggable = true
Toggle.MouseButton1Click:connect(function()
    Library:ToggleUI()
end)

local Corner = Instance.new("UICorner")
Corner.Name = "Corner"
Corner.Parent = Toggle
